page{width: 100%;height: 100%}
.report-container{width: 100%;height: 100%;padding: 30rpx;box-sizing: border-box;}
.report-container textarea{width: 100%;height: 400rpx;border:1px solid #dedede;padding: 10rpx;line-height: 40rpx;box-sizing: border-box;color:#333;margin-bottom: 40rpx;}
.report-container button{width: 100%;height: 80rpx;border-radius: 5px;background: #37b5fc;color: #fff;text-align: center;line-height: 80rpx;font-size: 36rpx;padding: 0;}
.report-container input{width: 100%;height: 80rpx;line-height: 80rpx;border:1px solid #dedede;padding: 0 10rpx;box-sizing: border-box;margin-bottom: 20rpx;}
.report-container picker{
  width: 100%;height: 80rpx;line-height: 80rpx;color:#333;border:1px solid #dedede;padding: 0 10rpx;box-sizing: border-box;margin-bottom: 20rpx;
}
