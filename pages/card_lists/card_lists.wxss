page{height: 100%;}
.cardlist_container{width: 100%;height: 100%;}
.card_lists{width: 100%;height: auto;padding: 0 30rpx;box-sizing: border-box;}
.card_block{width: 100%;height: 100rpx;padding: 0 30rpx;box-sizing: border-box;line-height: 100rpx;border-bottom: 1rpx solid #f5f5f5;}
.delete{width: 120rpx;height: 60rpx;font-size: 28rpx;line-height: 60rpx;text-align: center;color: #fff;background: #37b5fc;border-radius: 30rpx;float: right;margin-top: 20rpx;}
.empty_block{width: 100%;height: 100%;color: #999;font-size: 28rpx;text-align: center;line-height: 100vh;}